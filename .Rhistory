siqr     <- c(siqr,     NA)
medie    <- c(medie,    NA)
varianze <- c(varianze, NA)
next
}
x <- na.omit(df_s[[feat]])
if (length(x)==0 || max(x, na.rm=TRUE)==0) {
x_norm <- rep(0, length(x))
} else {
x_norm <- x / max(x, na.rm=TRUE)
}
mediane  <- c(mediane,  median(x_norm, na.rm=TRUE))
siqr     <- c(siqr,     SIQR(x_norm))
medie    <- c(medie,    mean(x_norm, na.rm=TRUE))       # **nuovo**
varianze <- c(varianze, var(x_norm, na.rm=TRUE))        # **nuovo**
}
# qui calcoliamo le 4 varianze inter‑scenario
