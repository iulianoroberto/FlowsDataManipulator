var_siqr      <- var(siqr,      na.rm=TRUE)
var_media     <- var(medie,     na.rm=TRUE)  # **nuovo**
var_varianza  <- var(varianze,  na.rm=TRUE)  # **nuovo**
risultati <- rbind(risultati,
data.frame(
Feature        = feat,
Var_Mediana    = var_median,
Var_SIQR       = var_siqr,
Var_Media      = var_media,      # **nuovo**
Var_Varianza   = var_varianza    # **nuovo**
))
}
